,GAAA,CAAA,KAAA,EAFA,CAGA;;AACA,UAAA,OAAA,GAAA;AACA,QAAA,OAAA,EAAA,KAAA,WAAA,CAAA,OADA;AAEA,QAAA,KAAA,EAAA,KAAA,WAAA,CAAA,KAFA;AAGA,QAAA,IAAA,EAAA,KAAA,WAAA,CAAA,IAHA;AAIA,QAAA,IAAA,EAAA,KAAA,WAAA,CAAA,IAJA;AAKA,QAAA,YAAA,EAAA,KAAA,WAAA,CAAA,YALA;AAMA,QAAA,WAAA,EAAA,KAAA,WAAA,CAAA,WANA;AAOA,QAAA,WAAA,EAAA,KAAA,WAAA,CAAA,WAPA;AAQA,QAAA,QAAA,EAAA,KAAA,WAAA,CAAA,QARA;AASA,QAAA,QAAA,EAAA,KAAA,WAAA,CAAA,QATA;AAUA,QAAA,MAAA,EAAA,KAAA,WAAA,CAAA,MAVA;AAWA,QAAA,MAAA,EAAA,KAAA,WAAA,CAAA;AAXA,OAAA;AAaA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAAA,OAAA,CAAA,OAAA;AACA,WAAA,QAAA;AACA,KAlGA;AAmGA,IAAA,cAnGA,0BAmGA,GAnGA,EAmGA;AACA,MAAA,GAAA,CAAA,cAAA;AACA,WAAA,KAAA,CAAA,aAAA,CAAA,IAAA;AACA,UAAA,IAAA,GAAA,KAAA;AACA,UAAA,KAAA,QAAA,CAAA,IAAA,CAAA,CAAA,CAAA,EAAA,IAAA,GAAA,IAAA;AACA,UAAA,OAAA,GAAA;AACA,QAAA,KAAA,EAAA,KAAA,QAAA,CAAA,KADA;AAEA,QAAA,MAAA,EAAA,KAAA,QAAA,CAAA,MAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA;AAKA,WAAA,UAAA,CAAA,OAAA,EAAA,KAAA,QAAA,CAAA,EAAA;AACA,KA9GA;AA+GA,IAAA,OA/GA,mBA+GA,GA/GA,EA+GA;AACA,MAAA,GAAA,CAAA,cAAA;AACA,WAAA,KAAA,CAAA,YAAA,CAAA,IAAA;AACA,WAAA,QAAA;AACA,KAnHA;AAoHA,IAAA,aApHA,yBAoHA,GApHA,EAoHA;AACA,MAAA,GAAA,CAAA,cAAA;AACA,WAAA,KAAA,CAAA,aAAA,CAAA,IAAA;AACA,WAAA,QAAA;AACA,WAAA,QAAA,GAJA,CAIA;AACA;AAzHA,GA9BA;AAyJA,EAAA,OAzJA,qBAyJA;AACA,SAAA,QAAA;AACA;AA3JA,CAAA","sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"row\">\n      <div class=\"col-sm-10\">\n        <h1>Bookings</h1>\n        <hr><br><br>\n        <alert :message=message v-if=\"showMessage\"></alert>\n        <br><br>\n        <c-modal ref=\"addBookModal\"\n            id=\"book-modal\"\n            title=\"Add a new book\"\n            hide-footer>\n        <b-form @submit=\"onSubmit\" @reset=\"onReset\" class=\"w-50\">\n        <b-form-group id=\"form-country-group\"\n                    label=\"Country:\"\n                    label-for=\"form-country-input\">\n          <b-form-input id=\"form-country-input\"\n                        type=\"text\"\n                        v-model=\"addBookForm.country\"\n                        required\n                        placeholder=\"Enter country\">\n          </b-form-input>\n        </b-form-group>\n        <b-form-group id=\"form-state-group\"\n                      label=\"State:\"\n                      label-for=\"form-state-input\">\n            <b-form-input id=\"form-state-input\"\n                          type=\"text\"\n                          v-model=\"addBookForm.state\"\n                          required\n                          placeholder=\"Enter state\">\n            </b-form-input>\n        </b-form-group>\n        <b-form-group id=\"form-city-group\"\n                      label=\"City:\"\n                      label-for=\"form-city-input\">\n            <b-form-input id=\"form-city-input\"\n                          type=\"text\"\n                          v-model=\"addBookForm.city\"\n                          required\n                          placeholder=\"Enter city\">\n            </b-form-input>\n        </b-form-group>\n        <b-form-group id=\"form-lots-group\"\n                      label=\"Lots:\"\n                      label-for=\"form-lots-input\">\n            <b-form-input id=\"form-lots-input\"\n                          type=\"text\"\n                          v-model=\"addBookForm.lots\"\n                          required\n                          placeholder=\"Enter lots\">\n            </b-form-input>\n        </b-form-group>\n        <b-form-group id=\"form-customertype-group\"\n                      label=\"Customer Type:\"\n                      label-for=\"form-customertype-input\">\n            <b-form-input id=\"form-customertype-input\"\n                          type=\"text\"\n                          v-model=\"addBookForm.customertype\"\n                          required\n                          placeholder=\"Enter customer type\">\n            </b-form-input>\n        </b-form-group>\n        <b-form-group id=\"form-vehicletype-group\"\n                      label=\"Vehicle Type:\"\n                      label-for=\"form-vehicletype-input\">\n            <b-form-input id=\"form-vehicletype-input\"\n                          type=\"text\"\n                          v-model=\"addBookForm.vehicletype\"\n                          required\n                          placeholder=\"Enter vehicle type\">\n            </b-form-input>\n        </b-form-group>\n        <b-form-group id=\"form-parkingtype-group\"\n                      label=\"Parking Type:\"\n                      label-for=\"form-parkingtype-input\">\n            <b-form-input id=\"form-parkingtype-input\"\n                          type=\"text\"\n                          v-model=\"addBookForm.parkingtype\"\n                          required\n                          placeholder=\"Enter parkingtype\">\n            </b-form-input>\n        </b-form-group>\n        <b-form-group id=\"form-fromdate-group\"\n                      label=\"From:\"\n                      label-for=\"form-fromdate-input\">\n            <b-form-input id=\"form-fromdate-input\"\n                          type=\"text\"\n                          v-model=\"addBookForm.fromdate\"\n                          required\n                          placeholder=\"Enter date\"\n                          style=\"width:40%;display:initial;\">\n            </b-form-input>\n            <b-form-input id=\"form-fromtime-input\"\n                          type=\"text\"\n                          v-model=\"addBookForm.fromtime\"\n                          required\n                          placeholder=\"Enter time\"\n                          style=\"width:40%;display:initial;\">\n            </b-form-input>\n        </b-form-group>\n        <b-form-group id=\"form-todate-group\"\n                      label=\"To:\"\n                      label-for=\"form-todate-input\">\n            <b-form-input id=\"form-todate-input\"\n                          type=\"text\"\n                          v-model=\"addBookForm.todate\"\n                          required\n                          placeholder=\"Enter date\"\n                          style=\"width:40%;display:initial;\">\n            </b-form-input>\n            <b-form-input id=\"form-totime-input\"\n                          type=\"text\"\n                          v-model=\"addBookForm.totime\"\n                          required\n                          placeholder=\"Enter time\"\n                          style=\"width:40%;display:initial;\">\n            </b-form-input>\n        </b-form-group>\n        <b-button-group>\n          <b-button type=\"submit\" variant=\"primary\">Submit</b-button>\n          <b-button type=\"reset\" variant=\"danger\">Reset</b-button>\n        </b-button-group>\n      </b-form>\n      </c-modal>\n      <br><br>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport Alert from './Alert.vue';\n\nexport default {\n  data() {\n    return {\n      books: [],\n      addBookForm: {\n        country: '',\n        state: '',\n        city: '',\n        lots: '',\n        customertype: '',\n        vehicletype: '',\n        parkingtype: '',\n        fromdate: '',\n        fromtime: '',\n        todate: '',\n        totime: '',\n      },\n      message: '',\n      showMessage: false,\n      editForm: {\n        id: '',\n        title: '',\n        author: '',\n        read: [],\n      },\n    };\n  },\n  components: {\n    alert: Alert,\n  },\n  methods: {\n    getBooks() {\n      const path = 'http://localhost:5000/bookings';\n      axios.get(path)\n        .then((res) => {\n          this.books = res.data.books;\n        // console.log(res.data.books);\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.error(error);\n        });\n    },\n    addBook(payload) {\n      const path = 'http://localhost:5000/bookings';\n      axios.post(path, payload)\n        .then(() => {\n          // this.getBooks();\n          this.message = 'Booking Done!';\n          this.showMessage = true;\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.log(error);\n          this.getBooks();\n        });\n    },\n    editBook(book) {\n      this.editForm = book;\n    },\n    updateBook(payload, bookID) {\n      const path = `http://localhost:5000/bookings/${bookID}`;\n      axios.put(path, payload)\n        .then(() => {\n          this.getBooks();\n          this.message = 'Book updated!';\n          this.showMessage = true;\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.error(error);\n          this.getBooks();\n        });\n    },\n    removeBook(bookID) {\n      const path = `http://localhost:5000/bookings/${bookID}`;\n      axios.delete(path)\n        .then(() => {\n          this.getBooks();\n          this.message = 'Book removed!';\n          this.showMessage = true;\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.error(error);\n          this.getBooks();\n        });\n    },\n    onDeleteBook(book) {\n      this.removeBook(book.id);\n    },\n    initForm() {\n      this.addBookForm.country = '';\n      this.addBookForm.state = '';\n      this.addBookForm.city = '';\n      this.addBookForm.lots = '';\n      this.addBookForm.customertype = '';\n      this.addBookForm.vehicletype = '';\n      this.addBookForm.parkingtype = '';\n      this.addBookForm.fromdate = '';\n      this.addBookForm.fromtime = '';\n      this.addBookForm.todate = '';\n      this.addBookForm.totime = '';\n      this.editForm.id = '';\n      this.editForm.title = '';\n      this.editForm.author = '';\n      this.editForm.read = [];\n    },\n    onSubmit(evt) {\n      evt.preventDefault();\n      console.log('abc');\n      // this.$refs.addBookModal.hide();\n      const payload = {\n        country: this.addBookForm.country,\n        state: this.addBookForm.state,\n        city: this.addBookForm.city,\n        lots: this.addBookForm.lots,\n        customertype: this.addBookForm.customertype,\n        vehicletype: this.addBookForm.vehicletype,\n        parkingtype: this.addBookForm.parkingtype,\n        fromdate: this.addBookForm.fromdate,\n        fromtime: this.addBookForm.fromtime,\n        todate: this.addBookForm.todate,\n        totime: this.addBookForm.totime,\n      };\n      console.log(payload);\n      this.addBook(payload);\n      this.initForm();\n    },\n    onSubmitUpdate(evt) {\n      evt.preventDefault();\n      this.$refs.editBookModal.hide();\n      let read = false;\n      if (this.editForm.read[0]) read = true;\n      const payload = {\n        title: this.editForm.title,\n        author: this.editForm.author,\n        read,\n      };\n      this.updateBook(payload, this.editForm.id);\n    },\n    onReset(evt) {\n      evt.preventDefault();\n      this.$refs.addBookModal.hide();\n      this.initForm();\n    },\n    onResetUpdate(evt) {\n      evt.preventDefault();\n      this.$refs.editBookModal.hide();\n      this.initForm();\n      this.getBooks(); // why?\n    },\n  },\n  created() {\n    this.getBooks();\n  },\n};\n</script>\n"],"sourceRoot":"src/components"}]}