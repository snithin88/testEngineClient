AAA,CAAA,WAAA,GAAA,OAAA,CAAA,KAAA,CAAA,WAAA,EAAA,MAAA,CAAA,CAAA;AAAA,eAAA,CAAA;AACA,aAFA;;AAGA,gBAAA,cAAA,GAAA,WAAA,CAAA,MAAA,CAAA,QAAA,EAAA,MAAA,CAAA;AACA,gBAAA,IAAA,GAAA,cAAA,CAAA,MAAA;AACA,gBAAA,IAAA,GAAA,IAAA,GAAA,CAAA;AACA,gBAAA,cAAA,GAAA,EAAA;AACA,gBAAA,WAAA,GAAA,EAAA;;AACA,iBAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,IAAA,IAAA,CAAA,GAAA,IAAA,EAAA,CAAA,IAAA,CAAA,EAAA,CAAA,IAAA,CAAA,EAAA;AACA,kBAAA,GAAA,GAAA,cAAA,CAAA,CAAA,CAAA;AACA,kBAAA,OAAA,GAAA,GAAA,CAAA,cAAA,CAAA,CAAA,CAAA,CAAA,CAAA,SAAA,CAAA;AACA,kBAAA,QAAA,GAAA,GAAA,CAAA,cAAA,CAAA,CAAA,CAAA,CAAA,CAAA,UAAA,CAAA;AACA,kBAAA,KAAA,GAAA,GAAA,CAAA,cAAA,CAAA,CAAA,CAAA,CAAA,CAAA,OAAA,CAAA;AACA,kBAAA,KAAA,GAAA,GAAA,CAAA,iBAAA,CAAA;;AACA,kBAAA,CAAA,GAAA,IAAA,EAAA;AACA,gBAAA,cAAA,IAAA,OAAA,OAAA,GAAA,GAAA,GAAA,QAAA,GAAA,IAAA,GAAA,KAAA,GAAA,MAAA,GAAA,KAAA,GAAA,GAAA;AACA,eAFA,MAGA;AACA,gBAAA,cAAA,IAAA,OAAA,OAAA,GAAA,GAAA,GAAA,QAAA,GAAA,IAAA,GAAA,KAAA,GAAA,MAAA;AACA;;AACA,cAAA,MAAA,CAAA,WAAA,GAAA,cAAA;AACA;;AACA,gBAAA,UAAA,GAAA,GAAA,CAAA,QAAA,CAAA,CAAA,MAAA,CAAA;AACA,gBAAA,WAAA,GAAA,GAAA,CAAA,QAAA,CAAA,CAAA,OAAA,CAAA;AACA,gBAAA,SAAA,GAAA,EAAA;;AACA,gBAAA,UAAA,KAAA,YAAA,EAAA;AACA,kBAAA,WAAA,GAAA,CAAA,EAAA;AACA,gBAAA,MAAA,CAAA,SAAA,GAAA,aAAA,KAAA,GAAA,KAAA,GAAA,UAAA,GAAA,KAAA,GAAA,WAAA,GAAA,GAAA,GAAA,IAAA;AACA,eAFA,MAGA;AACA,gBAAA,WAAA,GAAA,WAAA,GAAA,CAAA,CAAA;AACA,gBAAA,MAAA,CAAA,SAAA,GAAA,aAAA,KAAA,GAAA,KAAA,GAAA,UAAA,GAAA,KAAA,GAAA,WAAA,GAAA,GAAA,GAAA,IAAA;AACA;AACA,aARA,MASA,IAAA,UAAA,KAAA,MAAA,EAAA;AACA,kBAAA,WAAA,GAAA,CAAA,EAAA;AACA,gBAAA,MAAA,CAAA,SAAA,GAAA,aAAA,KAAA,GAAA,WAAA;AACA,eAFA,MAGA;AACA,gBAAA,WAAA,GAAA,WAAA,GAAA,CAAA,CAAA;AACA,gBAAA,MAAA,CAAA,SAAA,GAAA,aAAA,KAAA,GAAA,WAAA;AACA;AACA,aARA,MASA,IAAA,UAAA,KAAA,cAAA,EAAA;AACA,cAAA,MAAA,CAAA,SAAA,GAAA,WAAA;AACA;AACA;AACA,SAlEA;AAmEA,QAAA,MAAA,CAAA,KAAA,GAAA,IAAA;AACA,OA1EA,EA2EA,KA3EA,CA2EA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,KAAA;AACA,OA7EA;AA8EA;AAxHA,GArBA;AA+IA;AACA,EAAA,OAhJA,qBAgJA;AACA,SAAA,QAAA;AACA;AAlJA,CAAA","sourcesContent":["<template>\n  <div class=\"container\" style=\"max-width:100%;padding:0px;height:100%;\">\n <!--  <leftNav></leftNav>   Left side Navigation -->\n    <div class=\"row\"\n    style=\"margin-left: 280px;border-left: 18px solid #f8f9fa;height:100%\">\n      <div class=\"col-sm-10\" style=\"padding-left:30px;\">\n        <br>\n        <h4>Business Rules</h4>\n        <hr>\n        <div>\n  <b-navbar type=\"light\" variant=\"light\">\n    <!-- Search rules by lotid Form -->\n    <b-nav-form @submit=\"onSubmit\">\n      <b-form-input class=\"mr-sm-2\" placeholder=\"Search by Lot ID\" v-model=\"searchLot.name\"></b-form-input>\n      <b-button variant=\"outline-primary\" class=\"my-2 my-sm-0\" type=\"submit\">Search</b-button>\n    </b-nav-form>\n  </b-navbar>\n</div>\n        <br><br>\n        <!-- Rules display table -->\n        <table class=\"table table-hover\" style=\"width:75%\">\n          <thead>\n            <tr>\n              <th>S.No</th>\n              <th scope=\"col\">Rules</th>\n              <th>Actions</th>\n            </tr>\n          </thead>\n          <tbody id=\"rulestable\">\n            <tr v-for=\"(rule, key) in rules\" :key=\"key\" >\n              <td>{{ key+1 }}</td>\n              <td>{{ rule }}</td>\n              <td>\n                <div class=\"btn-group\" role=\"group\">\n                  <button\n                          type=\"button\"\n                          class=\"btn btn-warning btn-sm\"\n                          v-b-modal.if-condition-modal\n                          @mouseover=\"rulesfunction(rule)\"\n                          style =\"margin-right:20px;\">\n                      View\n                  </button>\n                  <button\n                          type=\"button\"\n                          class=\"btn btn-warning btn-sm\"\n                          v-b-modal.if-condition-modal\n                          style =\"margin-right:20px;\"\n                          @click=\"$router.push({name: 'editRules', query: { page: rule } })\">\n                      Edit\n                  </button>\n                  <button\n                          type=\"button\"\n                          class=\"btn btn-danger btn-sm\"\n                          v-b-modal.if-condition-modal\n                          @click=\"rulesfunction(rule)\"\n                          style =\"margin-right:20px;\">\n                      Delete\n                  </button>\n                </div>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n        <br>\n        <!-- If Conditions display based on rules -->\n        <div id=\"ifstate\" style=\"display:none\">\n            <div style=\"box-shadow: rgba(108, 117, 125, 0.25) 0px 0px 0px 5px inset; \n            width: 965px; padding: 30px 10px 30px 30px; min-height: 30px; margin-bottom: 30px;\n            margin-top: 20px;\">\n            <p><b><u>Conditions for {{ rulename }}</u></b></p>\n            <p>This rule is applicable for lots - <b>{{ lotdetails }}</b> and is valid\n            between <b>{{ startvalid }}</b> and <b>{{ stopvalid }}</b> </p>\n            <p><b>IF</b> ( {{ ifstatement }} )</p>\n            <p><b>THEN STALL RATE = </b>{{ finalrate }}</p>\n            </div>\n          </div>\n      </div>\n    </div>\n   <div>\n    </div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport moment from 'moment';\nimport Alert from './Alert.vue';\nimport leftNav from './leftNav.vue';\n\nexport default {\n  data() {\n    return {\n      rules: [],\n      finalrate: '',\n      ifstatement: '',\n      message: '',\n      showMessage: false,\n      rulename: '',\n      startvalid: '',\n      stopvalid: '',\n      lotdetails: '',\n      searchLot: {\n        name: '',\n      },\n    };\n  },\n  components: {\n    alert: Alert,\n    leftNav: leftNav,\n  },\n  methods: {\n    // Search rules by lotid\n    onSubmit(evt) {\n      evt.preventDefault();\n      document.querySelector('#ifstate').style.display = 'none';\n      const payload =  {\n        lotname: this.searchLot.name,\n      };\n      const path = 'http://localhost:5000/search'\n      axios.post(path,payload)\n        .then((res) => {\n          this.message = '';\n          this.rules = res.data;\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    },\n    // get all rules for display\n    getrules() {\n      const path = 'http://localhost:5000/busrules';\n      axios.post(path)\n        .then((res) => {\n          this.message = '';\n          this.showMessage = true;\n          this.rules = [];\n          const rule = [];\n          Object.keys(res.data).forEach((key) => {\n            rule.push(key);\n          });\n          this.rules = rule;\n          this.rows = this.rules.length;\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    },\n    // display a particular rule with if conditions\n    rulesfunction(name) {\n      const path = 'http://localhost:5000/busrules';\n      this.rulename = name;\n      document.querySelector('#ifstate').style.display = 'block';\n      axios.post(path)\n        .then((res) => {\n          this.message = '';\n          this.showMessage = true;\n          this.mainkeys = [];\n          const mainkeys1 = [];\n          Object.keys(res.data).forEach((key) => {\n            if (name === key) {\n              const val = res.data[key];\n              Object.keys(val).forEach((key1) => {\n                const val1 = val[key1];\n                if (key1 === 'validity') {\n                  const startvalid1 = val1.start;\n                  this.startvalid = moment(startvalid1).format('DD-MM-YYYY');\n                  const stopvalid1 = val1.end;\n                  this.stopvalid = moment(stopvalid1).format('DD-MM-YYYY');\n                }\n                if (key1 === 'lot') {\n                  const lotarray = val1;\n                  this.lotdetails = lotarray.toString();\n                }\n                mainkeys1.push(key1);\n                this.mainkeys = mainkeys1;\n              });\n              const val2 = ['cond1', 'cond2', 'cand1'];\n              const filterItems = (arr, query) => {\n              return arr.filter(el => el.toLowerCase().indexOf(query.toLowerCase()) !== -1);\n              }\n              const mainconditions = filterItems(this.mainkeys, 'cond' );\n              const len1 = mainconditions.length;\n              const len2 = len1 - 1;\n              let finalcondition = '';\n              const ifstatement = '';\n              for (let i = 0,j = 0; i < len1 || j < len2; i += 1, j +=1) {\n                const abc = mainconditions[i];\n                const operand = val[mainconditions[i]]['operand'];\n                const operator = val[mainconditions[i]]['operator'];\n                const value = val[mainconditions[i]]['value'];\n                const logop = val['logicalOperator'];\n                if (i < len2) {\n                  finalcondition += '( ' + operand + ' ' + operator + ' (' + value + ') ) ' + logop + ' ';\n                }\n                else {\n                  finalcondition += '( ' + operand + ' ' + operator + ' (' + value + ') ) ';\n                }\n                this.ifstatement = finalcondition;\n              }\n              const actiontype = val['action']['type'];\n              let actionvalue = val['action']['value'];\n              let finalrate = '';\n              if (actiontype === 'percentage') {\n                if (actionvalue > 0) {\n                  this.finalrate = 'baserate' + ' + ' + ' ( ' + 'baserate' + ' * ' + actionvalue + '%' + ' )';\n                }\n                else {\n                  actionvalue = actionvalue * (-1);\n                  this.finalrate = 'baserate' + ' - ' + ' ( ' + 'baserate' + ' * ' + actionvalue + '%' + ' )';\n                }\n              }\n              else if (actiontype === 'flat') {\n                if (actionvalue > 0) {\n                  this.finalrate = 'baserate' + ' + ' +  actionvalue;\n                }\n                else {\n                  actionvalue = actionvalue * (-1);\n                  this.finalrate = 'baserate' + ' - ' +  actionvalue;\n                }\n              }\n              else if (actiontype === 'specialprice') {\n                this.finalrate = actionvalue;\n              }\n            }\n          });\n          this.rules = rule;\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    },\n  },\n  // display all rules onload\n  created() {\n    this.getrules();\n  },\n};\n</script>\n"],"sourceRoot":"src/components"}]}