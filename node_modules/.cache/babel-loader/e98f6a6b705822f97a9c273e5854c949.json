urcesContent":["<template>\n  <div class=\"container\" style=\"max-width:100%;padding:0px;height:100%;\">\n <!--  <leftNav></leftNav>   Left side Navigation -->\n    <div class=\"row\"\n    style=\"margin-left: 280px;border-left: 18px solid #f8f9fa;height:100%\">\n      <div class=\"col-sm-10\" style=\"padding-left:30px;\">\n        <br>\n        <h4>Rules</h4>\n        <hr>\n        <alert :message=message v-if=\"showMessage\"></alert>\n        <br><br>\n        <!-- Add Rules Form -->\n    <v-modal ref=\"addRulesModal\"\n            id=\"rules-modal\"\n            title=\"Add a new rule\"\n            hide-footer>\n        <div class=\"addrules\">\n      <b-form @submit=\"onSubmit\" class=\"w-50\">\n        <div style=\"display: -webkit-inline-box;margin-right: 20px;\n        box-shadow: rgba(108, 117, 125, 0) 0px 0px 0px 5px inset;width:1150px;\n        padding: 30px 10px 30px 30px;min-height: 30px;margin-bottom: 30px;background-color:mintcream;\">\n        <b-form-group id=\"form-businessrule-group\"\n                    label=\"Business Rule:\"\n                    label-for=\"form-businessrule-input\"\n                    style=\"margin-right: 20px;\">\n          <b-form-input id=\"form-businessrule-input\"\n                        type=\"text\"\n                        v-model=\"addRulesForm.businessrule\" name=\"addRulesForm[0][businessrule]\"\n                        placeholder=\"Enter Business Rule\"\n                        >\n          </b-form-input>\n        </b-form-group>\n        <b-form-group id=\"form-fromdate-group\"\n                      label=\"Validity: From Date\"\n                      label-for=\"form-fromdate-input\"\n                      style=\"margin-right: 20px;width: 176px;\">\n            <Datepicker format=\"YYYY-MM-DD\" style=\"display: inline-block;\n            min-width: 176px;height: 38px;\"\n            \n            v-model=\"addRulesForm.fromdate\"\n            name=\"addRulesForm[0][fromdate]\"\n            placeholder=\"Enter date\"></Datepicker>\n        </b-form-group>\n        <b-form-group id=\"form-todate-group\"\n                      label=\"To Date\"\n                      label-for=\"form-todate-input\"\n                      style=\"margin-right: 20px;width: 176px;\">\n            <Datepicker format=\"YYYY-MM-DD\" style=\"display: inline-block;\n            min-width: 176px;height: 38px;\"\n            \n            v-model=\"addRulesForm.todate\"\n            name=\"addRulesForm[0][todate]\"\n            placeholder=\"Enter date\"></Datepicker>\n        </b-form-group>\n      <!--  <b-form-group label=\"City:\"\n                      label-for=\"form-city-input\"\n                      style=\"margin-right: 20px;\">\n            <b-form-select v-model=\"addRulesForm.city\"\n            id=\"form-city-group\"\n            name=\"addRulesForm[0][city]\" >\n            <option value=\"\" selected>Please select city</option>\n            <option v-for=\"(city, index) in cities\" :key=\"index\" :value=city>\n            {{city}}</option>\n        </b-form-select>\n        </b-form-group> -->\n        <b-form-group id=\"form-lot-group\"\n                    label=\"Lot:\"\n                    label-for=\"form-lot-input\"\n                    style=\"margin-right: 20px;width:300px;\">\n          <multiselect\n            v-model=\"addRulesForm.lot\"\n            :options=options\n            :multiple=\"true\"\n            name=\"addRulesForm[0][lot]\"\n            style=\"max-height: 30px;\">\n          </multiselect>\n        </b-form-group>\n        </div>\n        <!-- Conditions Form -->\n        <div\n        style=\"margin-right: 20px;box-shadow: rgba(108, 117, 125, 0) 0px 0px 0px 5px inset;\n        width: 1150px;background-color:mintcream;\n        padding: 30px 10px 30px 30px;min-height: 30px;margin-bottom: 30px;\">\n        <div></div><h5 style=\"margin-bottom:20px;\">IF</h5>\n        <div style=\"display: -webkit-box;\" v-for=\"(rules, index) in addRulesForm\" :key=\"index\">\n        <b-form-group id=\"form-op1-group\"\n                      style=\"margin-right: 20px;\">\n            <b-form-select v-model=\"rules.op1\" id=\"form-operand-group\" \n            v-on:change=\"getOperators(index,rules.op1);getFunctions(rules.op1,index);\" :name=\"`addRulesForm[${index}][op1]`\" required>\n            <option value=\"\" selected>Please select pricing factor</option>\n            <option v-for=\"(pfactor, ind) in pricingfactors\" :key=\"ind\" :value=pfactor>\n            {{pfactor}}</option>\n            </b-form-select>\n        </b-form-group>\n        <b-form-group id=\"form-operator-group\"\n                      style=\"margin-right: 20px;\">\n            <b-form-select v-model=\"rules.operator\" id=\"form-operator-group\" \n            :name=\"`addRulesForm[${index}][operator]`\" required>\n            <option value=\"\" selected>Please select operator</option>\n          <!--  <option v-for=\"(opr, ind) in `operators${index}`\" :key=\"ind\" :value=opr> \n            <option v-for=\"(opr, ind) in operators1\" :key=\"ind\" :value=opr>\n            {{opr}}</option> -->\n            <option value=\"in\">In</option>\n            <option value=\"equals\">Equals</option>\n            <option value=\"greaterthan\">greaterthan</option>\n            <option value=\"greaterthan or equlato\">greaterthan or equlato</option>\n            <option value=\"lessthan\">lessthan</option>\n            <option value=\"lessthan or equalto\">lessthan or equalto</option>\n            <option value=\"notequals\">notequals</option>\n            <option value=\"string\"></option>\n            </b-form-select>\n        </b-form-group>\n        <b-form-group id=\"form-type-group\"\n                      style=\"margin-right: 20px;\">\n            <b-form-select v-model=\"rules.type\" id=\"form-type-group\" \n            :name=\"`addRulesForm[${index}][type]`\" required>\n            <option value=\"\" selected>Please select Function</option>\n            <option v-for=\"(func, ind1) in functions1[index]\" :key=\"ind1\" :value=func>\n            {{func}}</option>\n            </b-form-select>\n        </b-form-group>\n        <b-form-group id=\"form-value-group\"\n                      style=\"margin-right: 20px;\">\n            <b-form-input id=\"form-value-input\"\n                          type=\"text\"\n                          v-model=\"rules.value\" :name=\"`addRulesForm[${index}][value]`\"\n                          placeholder=\"Enter value\">\n            </b-form-input>\n          </b-form-group>\n        <b-form-group id=\"form-logoperator-group\"\n                      style=\"margin-right: 20px;\">\n            <b-form-select v-model=\"rules.logoperator\" id=\"form-logoperator-group\" \n            :name=\"`addRulesForm[${index}][logoperator]`\" >\n            <option value=\"and\" selected=\"selected\">AND</option>\n            <option value=\"or\" id=\"any\">OR</option>\n            </b-form-select>\n        </b-form-group>\n        <div v-if=\"index == 0\"> \n        <button @click=\"addmorerules(count)\" variant=\"none\"\n          style=\"width: 30px;margin-top:5px;margin-right: 10px;background:none;border:none;\"><img src=\"plus.jpg\" alt=\"+\" style=\"width:20px;\"></button>\n        </div>\n        <div v-if=\"count > 0 && index > 0\">  \n        <button @click=\"removerules(count,index)\" variant=\"none\"\n          style=\"width: 30px;margin-top:5px;margin-right: 30px;background:none;border:none;\"><img src=\"minus.png\" alt=\"+\" style=\"width:24px;\"></button>\n        </div>  \n        </div>\n        </div>\n        <div style=\"display: -webkit-box;margin-right: 20px;\n        box-shadow: rgba(108, 117, 125, 0) 0px 0px 0px 5px inset;\n        width: 1150px;background-color:mintcream;\n        padding: 30px 10px 30px 30px;\">\n        <h5 style=\"margin-right:20px;margin-top:5px;\">THEN</h5>\n        <b-form-group id=\"form-actiontype-group\"\n                      style=\"margin-right: 20px;\">\n          <b-form-select v-model=\"addRulesForm.actiontype\" :name=\"`addRulesForm[0][actiontype]`\"\n            id=\"form-actiontype-group\" required>\n            <option value=\"\" selected=\"selected\">Please select action type</option>\n            <option value=\"flat\">Flat</option>\n            <option value=\"percentage\">Percentage</option>\n            <option value=\"specialprice\">Special Price</option>\n          </b-form-select>\n        </b-form-group>\n        <b-form-group id=\"form-actionvalue-group\"\n                      style=\"margin-right: 20px;\">\n            <b-form-input id=\"form-actionvalue-input\"\n                          type=\"text\"\n                          v-model=\"addRulesForm.actionvalue\" :name=\"`addRulesForm[0][actionvalue]`\"\n                          placeholder=\"Enter action value\">\n            </b-form-input>\n          </b-form-group>\n          </div>\n        <!--  <div style=\"display: -webkit-box;\">\n          <div id=\"ifrules\" style=\"display:none;\" >\n        <h1 style=\"background: lightgray;font-size: 1.2rem;width: 460px;\n        padding-left: 35px;margin-bottom: 30px;margin-top: 30px;\">IF\n        <select v-model=\"addRulesForm.logicaloperator\"\n            name=\"`addRulesForm[0][logicaloperator]`\"\n            id=\"form-logicaloperator-group\">\n            <option value=\"none\" selected id=\"none\">NONE</option>\n            <option value=\"or\" id=\"any\">ANY</option>\n            <option value=\"and\">ALL</option>\n            </select>\n        OF THE IF RULES ARE MATCHED</h1>\n        </div>\n        </div>  -->\n        <b-button-group style=\"display:-webkit-inline-box;width:700px;\">\n          <b-button type=\"submit\" variant=\"primary\"\n          style=\"width: 150px;margin-right: 30px;margin-top:30px;\">Submit</b-button>\n        <!--  <b-button @click=\"addmorerules(count)\" variant=\"primary\"\n          style=\"width: 150px;margin-top:30px;margin-right: 30px;\">Add Conditions</b-button>\n          <b-button @click=\"removerules(count)\" variant=\"primary\"\n          style=\"width: 170px;margin-top:30px;margin-right: 170px;display:none;\"\n          id=\"remcond\">Remove Conditions</b-button>  -->\n        </b-button-group>\n      </b-form>\n      </div>\n    </v-modal>\n    <br><br>\n    <hr><br><br>\n    </div>\n    </div>\n    <div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Datepicker from 'vuejs-datetimepicker';\nimport axios from 'axios';\nimport Multiselect from 'vue-multiselect';\nimport Alert from './Alert.vue';\nimport leftNav from './leftNav.vue';\n\nexport default {\n  data: () => ({\n    addRulesForm: [\n      {\n        op1: '',\n        operator: '',\n        type: '',\n        value: '',\n        logoperator: 'and',\n        actionvalue: '',\n        actiontype: '',\n        businessrule: '',\n        lots: '',\n        fromdate: '',\n        todate: '',\n      },\n    ],\n    selected: null,\n    options: ['1491','1492'],\n    message: '',\n    showMessage: false,\n    count: 0,\n    operandlist: [],\n    pricingfactors: [],\n    operators: [],\n    operators1:[],\n    functions: [],\n    functions1: [],\n    operatorlist: {'in':'in', 'equals':'==', 'greaterthan':'>', 'greaterthan or equlato':'>=', 'lessthan':'<',\n     'lessthan or equalto':'<=', 'notequals':'!='},\n  }),\n  components: {\n    alert: Alert,\n    leftNav: leftNav,\n    Multiselect,\n    Datepicker,\n  },\n  methods: {\n    // Add more conditions\n    addmorerules(val) {\n      event.preventDefault();\n      const newval = parseInt(val, 10);\n      const val1 = val + 1;\n      const add = '#add';\n      const sub = '#remcond';\n      const ifrules = '#ifrules';\n      if (val1 >= 1) {\n       // document.querySelector(ifrules).style.display = 'block';\n       // document.querySelector(sub).style.display = 'block';\n      }\n     // document.getElementById('none').style.display = 'none';\n     // document.getElementById('form-logicaloperator-group').options.selectedIndex = '1';\n      this.addRulesForm.push({\n        op1: '',\n        operator: '',\n        type: '',\n        value: '',\n        action: '',\n        actionvalue: '',\n      });\n      this.count = val1;\n      if (newval >= 0) {\n        document.querySelector(`${add}${newval}`).style.display = 'none';\n        document.querySelector(`${sub}${newval}`).style.display = 'none';\n        document.querySelector(`${ifrules}${newval}`).style.display = 'none';\n      }\n    },\n    // remove conditions\n    removerules(val,remid) {\n      event.preventDefault();\n      const sub = '#remcond';\n      const ifrules = '#ifrules';\n      if (val === 1) {\n       // document.querySelector(ifrules).style.display = 'none';\n       // document.querySelector(sub).style.display = 'none';\n      }\n      this.addRulesForm.splice(remid, 1);\n      const val1 = val - 1;\n      this.count = val1;\n    },\n    // add new rules\n    addRules(payload) {\n      const path = 'http://localhost:5000/rules';\n      axios.post(path, payload)\n        .then(() => {\n          this.message = 'Rules Added Successfully';\n          this.showMessage = true;\n          window.scrollTo(0, 0);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    // Fetch operands from json\n    getOperands() {\n      const path = 'http://localhost:5000/listoperand';\n      let pricingfactors = '';\n      let operandlist = '';\n      axios.get(path)\n        .then((res) => {\n          this.operandlist = res.data;\n          this.pricingfactors = Object.keys(res.data);\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    },\n    // Fetch operators \n    getOperators(ind,oprval) {\n      const value = oprval;\n      Object.keys(this.operandlist).forEach((key) => {\n        const val = this.operandlist[key];\n        if (key === value) {\n          this.operators = this.operandlist[key]['operator'];\n          this.operators1 = this.operators;\n        }\n      });\n    },\n    // Fetch functions based on operands\n    getFunctions(oprname,id) {\n      const value = oprname;\n          Object.keys(this.operandlist).forEach((key) => {\n            const val = this.operandlist[key];\n            if (key === value) {\n              this.functions1[id] = this.operandlist[key]['type'];\n            }\n          });\n    },\n    // Initializing rules form\n    initForm() {\n      this.addRulesForm.op1 = '';\n      this.addRulesForm.operator = '';\n      this.addRulesForm.type = '';\n      this.addRulesForm.value = '';\n      this.addRulesForm.actionvalue = '';\n      this.addRulesForm.logoperator = '';\n    },\n    // Submit function of rules form\n    onSubmit(evt) {\n      evt.preventDefault();\n      const mainarray = this.addRulesForm;\n      const lot1 = this.addRulesForm.lot;\n      const fromdate1 = this.addRulesForm.fromdate;\n      const todate1 = this.addRulesForm.todate;\n      // let logicaloperator1 = this.addRulesForm.logicaloperator;\n      let formdata = {};\n      let arraydata = {};\n      let conditiondata = {};\n      \n      for (let i = 0; i < mainarray.length; i += 1) {\n        const operand1 = this.addRulesForm[i].op1;\n        let operator1 = this.addRulesForm[i].operator;\n        const type1 = this.addRulesForm[i].type;\n        let value1 = this.addRulesForm[i].value;\n        let logicaloperator1 = this.addRulesForm[i].logoperator;\n        if (mainarray.length === 1) {\n          logicaloperator1 = 'none';\n        }\n        value1 = value1.split(',');\n        formdata = {};\n        const cond = 'cond';\n        const conditions = `${cond}${i + 1}`;\n        const operand2 = 'operand';\n        const operator2 = 'operator';\n        const type2 = 'type';\n        const value2 = 'value';\n        formdata[conditions] = {};\n        formdata[conditions][operand2] = operand1;\n        formdata[conditions][operator2] = operator1;\n        formdata[conditions][type2] = type1;\n        formdata[conditions][value2] = value1;\n        formdata[conditions]['logoperator'] = logicaloperator1;\n        arraydata = Object.assign(arraydata, formdata);\n        conditiondata = Object.assign(conditiondata, formdata);\n      }\n\n      const conditionkeys = Object.keys(conditiondata);\n      const len1 = conditionkeys.length;\n      const len2 = len1 - 1;\n      let finalcondition = '';\n      for (let k = 0,j = 0; k < len1 || j < len2; k += 1, j +=1) {\n        const key1 = conditionkeys[k];\n        const operand = conditiondata[key1]['operand'];\n        const operator = conditiondata[key1]['operator'];\n        const value = conditiondata[key1]['value'];\n        const addFunction = conditiondata[key1]['type'];\n        const logop = logicaloperator1;\n        const vallen = value.length;\n        let newval = '';\n        const vallimit = vallen - 1;\n        for (let m = 0; m <  vallen; m += 1) {\n          if (m != vallimit) {\n            newval += '\"' + value[m] + '\"' + ', ';\n          }\n          else {\n            newval += '\"' + value[m] + '\"';\n          }\n        }\n        let operatorsign = '';\n        Object.keys(this.operatorlist).forEach((oprname) => {\n          if (oprname == operator) {\n            operatorsign = this.operatorlist[oprname];\n          }\n        });\n        if (addFunction == 'string') {\n          addFunction = '';\n        }\n        if (k < len2) {\n          finalcondition += '( ' + addFunction + '(' + operand + ')' + ' ' + operatorsign + ' ' + '(' + newval + ')' + ' ) ' + logop + ' ';\n        }\n        else {\n          finalcondition += '( ' + addFunction + '(' + operand + ')' + ' ' + operatorsign + ' ' + '(' + newval + ')' + ' ) ';\n        }\n      }\n      const lotlen = lot1.length;\n      const lotlimit = lotlen - 1;\n      let lotval = '';\n      for (let n = 0; n <  lotlen; n += 1) {\n          if (n != lotlimit) {\n            lotval += '\"' + lot1[n] + '\"' + ', ';\n          }\n          else {\n            lotval += '\"' + lot1[n] + '\"';\n          }\n      }\n      const validitycondition = '( ' + 'getCurrentDate() >= ' + '\"' + fromdate1 + '\"' + ' ' + 'and' + ' ' + 'getCurrentDate() <= ' + '\"' + todate1 + '\"' + ' )';\n      const lotcondition = '(lotid in (' + lotval + '))';  \n      finalcondition = '(' + lotcondition + ' and ' + validitycondition + ' and ('  + finalcondition + '))';\n      const lot2 = 'lot';\n      const validity2 = 'validity';\n      const start = 'start';\n      const end = 'end';\n      const action2 = 'action';\n      const name = 'name';\n      const type = 'type';\n      const value = 'value';\n      const logicaloperator2 = 'logicalOperator';\n      const actionvalue1 = this.addRulesForm.actionvalue;\n      const actiontype1 = this.addRulesForm.actiontype;\n      const rulename = this.addRulesForm.businessrule;\n      const finalarray1 = {};\n      finalarray1[rulename] = arraydata;\n      finalarray1[rulename][lot2] = lot1;\n      finalarray1[rulename][action2] = {};\n      finalarray1[rulename][action2][type] = actiontype1;\n      finalarray1[rulename][action2][value] = actionvalue1;\n      finalarray1[rulename][validity2] = {};\n      finalarray1[rulename][validity2][start] = fromdate1;\n      finalarray1[rulename][validity2][end] = todate1;\n      finalarray1[rulename]['enable'] = 'true';\n      finalarray1[rulename]['prioritytype'] = 'mandatory';\n      finalarray1[rulename]['priorityvalue'] = 0;\n      finalarray1[rulename]['querystring'] = finalcondition;\n      if (logicaloperator1 !== 'none') {\n        finalarray1[rulename][logicaloperator2] = logicaloperator1;\n      }\n      else {\n        finalarray1[rulename][logicaloperator2] = 'none';\n      }\n\n      const payload = finalarray1;\n      this.addRules(payload);\n      this.initForm();\n    },\n  },\n  // city and operands are listed onload\n  created() {\n   // this.getcity();\n    this.getOperands();\n  },\n};\n</script>\n<style src=\"vue-multiselect/dist/vue-multiselect.min.css\"></style>\n<style scoped>\n#tj-datetime-input {\n    height: 38px;\n    min-width: 176px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}