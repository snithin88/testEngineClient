console.log(this.count);\n      if (newval >= 0) {\n        document.querySelector(`${add}${newval}`).style.display = 'none';\n        document.querySelector(`${sub}${newval}`).style.display = 'none';\n        document.querySelector(`${ifrules}${newval}`).style.display = 'none';\n      }\n    },\n    removerules(val) {\n      const sub = '#remcond';\n      const ifrules = '#ifrules';\n      if (val === 1) {\n        document.querySelector(ifrules).style.display = 'none';\n        document.querySelector(sub).style.display = 'none';\n      }\n      this.addRulesForm.splice(0, 1);\n      const val1 = val - 1;\n      this.count = val1;\n    },\n    addRules(payload) {\n      const path = 'http://localhost:5000/rules';\n      axios.post(path, payload)\n        .then(() => {\n          this.message = 'Rules Added Successfully';\n          this.showMessage = true;\n          window.scrollTo(0, 0);\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.log(error);\n        });\n    },\n    getcity() {\n      console.log('bbbb');\n      const value1 = 'USA';\n      this.cities = [];\n      this.addRulesForm.city = '';\n      axios.get('country.json')\n        .then((response) => {\n          Object.keys(response.data.country).forEach((key) => {\n            const val = response.data.country[key];\n            const key1 = Object.keys(val);\n            const len1 = key1.length;\n            for (let i = 0; i < len1; i += 1) {\n              const key2 = key1[i];\n              const abc = response.data.country[key][key2];\n              if (abc === value1) {\n                this.cities = response.data.country[key].cities;\n              }\n            }\n          });\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.error(error);\n        });\n    },\n    getLots() {\n      console.log('aaaaa');\n      this.options = '';\n      const value1 = document.querySelector('#form-city-grou