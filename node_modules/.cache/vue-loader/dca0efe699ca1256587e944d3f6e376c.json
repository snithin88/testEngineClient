a;height:100%\">\n      <div class=\"col-sm-10\" style=\"padding-left:30px;\">\n        <br>\n        <h4>Promocode</h4>\n        <hr>\n        <alert :message=message v-if=\"showMessage\"></alert>\n        <c-modal ref=\"addPromoModal\"\n            id=\"promo-modal\"\n            title=\"Add a new promocode\"\n            hide-footer>\n        <b-form @submit=\"onSubmit\" class=\"w-50\">\n        <div style=\"margin-right: 20px;\n        box-shadow: rgba(108, 117, 125, 0.25) 0px 0px 0px 5px inset;width:1150px;\n        padding: 30px 10px 30px 30px;min-height: 30px;margin-bottom: 30px;\">\n        <div style=\"display: -webkit-inline-box;margin-bottom:10px;\">\n        <b-form-group id=\"form-name-group\"\n                      label=\"Promocode Name:\"\n                      label-for=\"form-name-input\"\n                      style=\"margin-right: 20px;\">\n            <b-form-input id=\"form-name-input\"\n                          type=\"text\"\n                          v-model=\"editPromoForm.name\"\n                          placeholder=\"Enter promo name\">\n            </b-form-input>\n          </b-form-group> \n        <b-form-group id=\"form-lot-group\"\n                    label=\"Lot:\"\n                    label-for=\"form-lot-input\"\n                    style=\"margin-right: 20px;width:300px;\">\n          <multiselect\n            v-model=\"editPromoForm.lotid\"\n            :options=\"options\"\n            :multiple=\"true\"\n            style=\"max-height: 30px;\">\n          </multiselect>\n        </b-form-group>\n        <b-form-group id=\"form-type-group\"\n                      label=\"Type:\"\n                      label-for=\"form-type-input\"\n                      style=\"margin-right: 20px;\">\n            <b-form-select v-model=\"editPromoForm.type\"\n            id=\"form-type-group\">\n            <option value=\"\" selected>Please select type</option>\n            <option value=\"flat\">Flat</option>\n            <option value=\"percentage\">%</option>\n             <option value=\"specialprice\">Special Price</option>\n            </b-form-select>\n            </b-form-group>\n         <b-form-group id=\"form-value-group\"\n                      label=\"Value:\"\n                      label-for=\"form-alue-input\"\n                      style=\"margin-right: 20px;\">\n            <b-form-input id=\"form-value-input\"\n                          type=\"text\"\n                          v-model=\"editPromoForm.value\"\n                          placeholder=\"Enter value\">\n            </b-form-input>\n          </b-form-group>    \n        <b-form-group id=\"form-stalls-group\"\n                      label=\"Number of Stalls:\"\n                      label-for=\"form-stalls-input\"\n                      style=\"margin-right: 20px;\">\n            <b-form-input id=\"form-stalls-input\"\n                          type=\"text\"\n                          v-model=\"editPromoForm.Numberofstalls\"\n                          placeholder=\"Enter stalls\">\n            </b-form-input>\n          </b-form-group>\n          </div>\n         <div style=\"display: -webkit-inline-box;margin-right: 20px;\">\n          <b-form-group id=\"form-maxduration-group\"\n                      label=\"Max Duration: (mins)\"\n                      label-for=\"form-maxduration-input\"\n                      style=\"margin-right: 20px;\">\n            <b-form-input id=\"form-maxduration-input\"\n                          type=\"text\"\n                          v-model=\"editPromoForm.MaxDuration\"\n                          placeholder=\"Enter maxduration\">\n            </b-form-input>\n          </b-form-group> \n        <b-form-group id=\"form-validfor-group\"\n                      label=\"Valid For:\"\n                      label-for=\"form-validfor-input\"\n                      style=\"margin-right: 20px;\">\n            <b-form-select v-model=\"editPromoForm.validfor\" id=\"form-validfor-group\" required>\n            <option value=\"\" selected>Please select</option>\n            <option value=\"transient\">Transient</option>\n            <option value=\"monthly\">Monthly</option>\n          </b-form-select>\n        </b-form-group>\n        <b-form-group id=\"form-fromdate-group\"\n                      label=\"start date:\"\n                      label-for=\"form-fromdate-input\"\n                      style=\"margin-right: 20px;\">\n            <Datepicker format=\"YYYY-MM-DD\" style=\"width: 100px;display: inline-block;margin-right: 20px;\"\n            required\n            v-model=\"editPromoForm.fromdate\"\n            placeholder=\"Enter date\"></Datepicker>\n            <vue-timepicker format=\"h:mm A\"\n            :minute-interval=\"15\"\n            close-on-complete\n            v-model=\"editPromoForm.fromtime\"\n            style=\"height: 30px;float: right;margin-left: 60px;\"\n            placeholder=\"Enter time\"\n            required>\n            </vue-timepicker>\n        </b-form-group>\n        <b-form-group id=\"form-todate-group\"\n                      label=\"end date:\"\n                      label-for=\"form-todate-input\"\n                      style=\"margin-right: 20px;\">\n            <Datepicker format=\"YYYY-MM-DD\" style=\"width: 100px;display: inline-block;\"\n            required\n            v-model=\"editPromoForm.todate\"\n            placeholder=\"Enter date\"></Datepicker>\n            <vue-timepicker format=\"h:mm A\"\n            :minute-interval=\"15\"\n            close-on-complete\n            v-model=\"editPromoForm.totime\"\n            style=\"height: 30px;float: right;margin-left: 80px;\"\n            placeholder=\"Enter time\"\n            required>\n            </vue-timepicker>\n        </b-form-group>\n        </div>\n        </div>\n        <b-button-group>\n          <b-button type=\"submit\" variant=\"primary\">Submit</b-button>\n        </b-button-group>\n      </b-form>\n      </c-modal>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport moment from 'moment';\nimport Datepicker from 'vuejs-datetimepicker';\nimport VueTimepicker from 'vue2-timepicker';\nimport 'vue2-timepicker/dist/VueTimepicker.css';\nimport Multiselect from 'vue-multiselect';\nimport axios from 'axios';\nimport Alert from './Alert.vue';\nimport leftNav from './leftNav.vue';\n\nexport default {\n  data() {\n    return {\n      editPromoForm: {\n        name: '',\n        lotid: '',\n        Numberofstalls: '',\n        fromdate: '',\n        fromtime: '',\n        todate: '',\n        totime: '',\n        value: '',\n        type: '',\n        validfor: '',\n        MaxDuration: '',\n      },\n      options: ['1491', '1492'],\n      message: '',\n      showMessage: false,\n    };\n  },\n  components: {\n    alert: Alert,\n    leftNav: leftNav,\n    Datepicker,\n    VueTimepicker,\n    Multiselect,\n  },\n  methods: {\n    getPromo() {\n      console.log(this.$route.query.page);\n      const promoid = this.$route.query.page;\n      const path = `http://localhost:5000/promoList/${promoid}`;\n      axios.get(path)\n        .then((response) => {\n          console.log(response.data);\n          this.editPromoForm = response.data;\n          this.editPromoForm.name = promoid;\n          this.editPromoForm.fromdate = moment(this.editPromoForm.startdate).format('YYYY-MM-DD');\n          this.editPromoForm.fromtime = moment(this.editPromoForm.startdate).format('h:mm A');\n          this.editPromoForm.todate = moment(this.editPromoForm.enddate).format('YYYY-MM-DD');\n          this.editPromoForm.totime = moment(this.editPromoForm.enddate).format('h:mm A');\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.error(error);\n        });  \n    },  \n    timeCalc(timevalue, datevalue) {\n      let hours = Number(timevalue.match(/^(\\d+)/)[1]);\n      const minutes = Number(timevalue.match(/:(\\d+)/)[1]);\n      const AMPM = timevalue.match(/\\s(.*)$/)[1];\n      if (AMPM === 'PM' && hours < 12) hours += 12;\n      if (AMPM === 'AM' && hours === 12) hours -= 12;\n      let sHours = hours.toString();\n      let sMinutes = minutes.toString();\n      if (hours < 10) sHours = `0${sHours}`;\n      if (minutes < 10) sMinutes = `0${sMinutes}`;\n      const seconds = '00';\n      const time1 = `${sHours}:${sMinutes}:${seconds}`;\n      const dateFromPicker = datevalue;\n      const timeFromPicker = time1;\n      const datetime = `${datevalue} ${time1}`;\n      const dateParts = dateFromPicker.split('-');\n      const timeParts = timeFromPicker.split(':');\n      console.log(datevalue);\n      console.log(time1);\n      const localDate = new Date(dateParts[0], dateParts[1] - 1,\n        dateParts[2], timeParts[0], timeParts[1]);\n      const timestamp = localDate.getTime();\n      console.log(localDate);\n      console.log(timestamp);\n      return datetime;\n    },\n    addPromo(payload) {\n      const path = 'http://localhost:5000/promocode';\n      console.log(path);\n      axios.post(path, payload)\n        .then((resp) => {\n          this.message = \"Promocode Edited successfully\";\n          this.showMessage = true;\n          window.scrollTo(0, 0);\n        //  setTimeout(window.scrollTo(0, 0), 500);\n          setTimeout( () => this.$router.push({ path: '/promodisplay'}), 1000);\n        //  this.$router.push('promodisplay');\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.log(error);\n        });\n    },\n    initForm() {\n      this.editPromoForm.name = '';\n      this.editPromoForm.lotid = '';\n      this.editPromoForm.Numberofstalls = '';\n      this.editPromoForm.fromdate = '';\n      this.editPromoForm.fromtime = '';\n      this.editPromoForm.todate = '';\n      this.editPromoForm.totime = '';\n      this.editPromoForm.MaxDuration = '';\n      this.editPromoForm.type = '';\n      this.editPromoForm.value = '';\n      this.editPromoForm.validfor = '';\n    },\n    onSubmit(evt) {\n      evt.preventDefault();\n      const promoarray = {};\n      const promoname = this.editPromoForm.name;\n      promoarray[promoname] = {};\n      promoarray[promoname]['lotid'] = this.editPromoForm.lotid;\n      promoarray[promoname]['type'] = this.editPromoForm.type;\n      promoarray[promoname]['value'] = this.editPromoForm.value;\n      promoarray[promoname]['startdate'] = this.timeCalc(this.editPromoForm.fromtime, this.editPromoForm.fromdate);\n      promoarray[promoname]['enddate'] = this.timeCalc(this.editPromoForm.totime, this.editPromoForm.todate);\n      promoarray[promoname]['Numberofstalls'] = this.editPromoForm.Numberofstalls;\n      promoarray[promoname]['MaxDuration'] = this.editPromoForm.MaxDuration;\n      promoarray[promoname]['validfor'] = this.editPromoForm.validfor;\n      console.log(promoarray);\n        this.addPromo(promoarray);\n        this.initForm();\n    },\n  },\n  created() {\n    this.getPromo();\n  },\n};\n</script>\n"]}]}