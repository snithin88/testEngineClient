"src/components","sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"row\">\n      <div class=\"col-sm-10\">\n        <h1>Bookings</h1>\n        <hr><br><br>\n        <alert :message=message v-if=\"showMessage\"></alert>\n        <c-modal ref=\"addBookModal\"\n            id=\"book-modal\"\n            title=\"Add a new book\"\n            hide-footer>\n        <b-form @submit=\"onSubmit\" class=\"w-50\">\n        <div style=\"margin-right: 20px;\n        box-shadow: rgba(108, 117, 125, 0.25) 0px 0px 0px 5px inset;width:1150px;\n        padding: 30px 10px 30px 30px;min-height: 30px;margin-bottom: 30px;\">\n        <div style=\"display: -webkit-inline-box;\">\n        <b-form-group id=\"form-lots-group\"\n                      label=\"Lots:\"\n                      label-for=\"form-lots-input\"\n                      style=\"margin-right: 20px;\">\n            <b-form-select v-model=\"addBookForm.lots\" id=\"form-lots-group\" required>\n            <option value=\"\" selected>Please select lots</option>\n            <option value=\"1491\">North Broadway</option>\n            <option value=\"1492\">Wrigleyville</option>\n          </b-form-select>\n        </b-form-group>\n        <b-form-group id=\"form-customertype-group\"\n                      label=\"Customer Type:\"\n                      label-for=\"form-customertype-input\"\n                      style=\"margin-right: 20px;\">\n            <b-form-select v-model=\"addBookForm.customertype\" id=\"form-customertype-group\" required>\n            <option value=\"\" selected>Please select Customer type</option>\n            <option value=\"visitor\">Visitor</option>\n            <option value=\"employee\">Employee</option>\n            <option value=\"resident\">Resident</option>\n            <option value=\"whitelisted\">Whitelisted</option>\n            <option value=\"staff\">Staff</option>\n            <option value=\"transientcustomer\">Transient Customer</option>\n            <option value=\"repeatcustomer\">Repeat Customer</option>\n            <option value=\"priviligedcustomer\">Priviliged Customer</option>\n    </b-form-select>\n        </b-form-group>\n        <b-form-group id=\"form-vehicletype-group\"\n                      label=\"Vehicle Type:\"\n                      label-for=\"form-vehicletype-input\"\n                      style=\"margin-right: 20px;\">\n            <b-form-select v-model=\"addBookForm.vehicletype\" id=\"form-vehicletype-group\" required>\n            <option value=\"\" selected>Please select Vehicle type</option>\n            <option value=\"twowheeler\">Two wheeler</option>\n            <option value=\"bicycle\">Bicycle</option>\n            <option value=\"sedan\">Sedan</option>\n            <option value=\"hatchback\">Hatch Back</option>\n            <option value=\"ambulance\">Ambulance</option>\n            <option value=\"bus\">Bus</option>\n            <option value=\"electricvehicle\">Electric Vehicle</option>\n    </b-form-select>\n        </b-form-group>\n        <b-form-group label=\"Parking Type:\"\n                      label-for=\"form-parkingtype-input\"\n                      style=\"margin-right: 20px;\">\n            <b-form-select v-model=\"addBookForm.parkingtype\" id=\"form-parkingtype-group\" required>\n            <option value=\"\" selected>Please select Parking type</option>\n            <option value=\"transient\">Transient Parking</option>\n            <option value=\"monthly\">Monthly</option>\n            <option value=\"event\">Events</option>\n    </b-form-select>\n        </b-form-group>\n        </div>\n         <div style=\"display: -webkit-inline-box;margin-right: 20px;\">\n        <b-form-group id=\"form-fromdate-group\"\n                      label=\"From:\"\n                      label-for=\"form-fromdate-input\"\n                      style=\"margin-right: 20px;\">\n            <Datepicker format=\"YYYY-MM-DD\" style=\"width: 100px;display: inline-block;margin-right: 20px;\"\n            required\n            v-model=\"addBookForm.fromdate\"\n            placeholder=\"Enter date\"></Datepicker>\n            <vue-timepicker format=\"h:mm A\"\n            :minute-interval=\"15\"\n            close-on-complete\n            v-model=\"addBookForm.fromtime\"\n            style=\"height: 30px;float: right;margin-left: 60px;\"\n            placeholder=\"Enter time\"\n            required>\n            </vue-timepicker>\n        </b-form-group>\n        <b-form-group id=\"form-todate-group\"\n                      label=\"To:\"\n                      label-for=\"form-todate-input\"\n                      style=\"margin-right: 20px;\">\n            <Datepicker format=\"YYYY-MM-DD\" style=\"width: 100px;display: inline-block;\"\n            required\n            v-model=\"addBookForm.todate\"\n            placeholder=\"Enter date\"></Datepicker>\n            <vue-timepicker format=\"h:mm A\"\n            :minute-interval=\"15\"\n            close-on-complete\n            v-model=\"addBookForm.totime\"\n            style=\"height: 30px;float: right;margin-left: 80px;\"\n            placeholder=\"Enter time\"\n            required>\n            </vue-timepicker>\n        </b-form-group>\n        <b-form-group id=\"form-interval-group\"\n                      label=\"Interval:\"\n                      label-for=\"form-interval-input\"\n                      style=\"margin-right: 20px;\">\n            <b-form-input id=\"form-interval-input\"\n                          type=\"text\"\n                          v-model=\"addBookForm.interval\"\n                          placeholder=\"Enter Interval\">\n            </b-form-input>\n          </b-form-group>\n        </div>\n        <b-form-checkbox v-model=\"addBookForm.checkedPromo\" style=\"margin-bottom:20px;\">\n          Click to add Promocode\n        </b-form-checkbox>\n        <div v-if=\"addBookForm.checkedPromo == true\">\n         <b-form-group label=\"Promo code:\"\n                      label-for=\"form-promocode-input\"\n                      style=\"width:250px;\">\n            <b-form-select v-model=\"addBookForm.promocode\" id=\"form-promocode-group\" required>\n            <option value=\"\" selected>Please select Promocode</option>\n            <option value=\"Promo10\">PROMO 10</option>\n            <option value=\"Promo20\">PROMO 20</option>\n            <option value=\"Promo50\">PROMO 50</option>\n    </b-form-select>\n        </b-form-group>\n        </div>\n        </div>\n        <b-button-group>\n          <b-button type=\"submit\" variant=\"primary\">Submit</b-button>\n        </b-button-group>\n      </b-form>\n      </c-modal>\n      <b-modal ref=\"bookingrangeModal\"\n            id=\"booking-range-modal\"\n            title=\"\"\n            hide-footer>\n          <div id=\"response\" class=\"tablemodal\"></div>\n    </b-modal>\n      <br><br>\n      <br><br>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Datepicker from 'vuejs-datetimepicker';\nimport VueTimepicker from 'vue2-timepicker';\nimport 'vue2-timepicker/dist/VueTimepicker.css';\nimport axios from 'axios';\nimport Alert from './Alert.vue';\n\nexport default {\n  data() {\n    return {\n      books: [],\n      states: [],\n      cities: [],\n      lotss: [],\n      defaultrate: '',\n      ratetype: '',\n      lowestrate: '',\n      durationinhrs: '',\n      numberofstalls: '',\n      totalrate: '',\n      addBookForm: {\n        country: '',\n        state: '',\n        city: '',\n        lots: '',\n        customertype: '',\n        vehicletype: '',\n        parkingtype: '',\n        stalls: '',\n        fromdate: '',\n        fromtime: '',\n        todate: '',\n        totime: '',\n        actualdate: '',\n        actualtime: '',\n        checkedPromo: false,\n        promocode: '',\n        interval: '',\n      },\n      message: '',\n      showMessage: false,\n    };\n  },\n  components: {\n    alert: Alert,\n    Datepicker,\n    VueTimepicker,\n  },\n  methods: {\n    timeCalc(timevalue, datevalue) {\n      let hours = Number(timevalue.match(/^(\\d+)/)[1]);\n      const minutes = Number(timevalue.match(/:(\\d+)/)[1]);\n      const AMPM = timevalue.match(/\\s(.*)$/)[1];\n      if (AMPM === 'PM' && hours < 12) hours += 12;\n      if (AMPM === 'AM' && hours === 12) hours -= 12;\n      let sHours = hours.toString();\n      let sMinutes = minutes.toString();\n      if (hours < 10) sHours = `0${sHours}`;\n      if (minutes < 10) sMinutes = `0${sMinutes}`;\n      const seconds = '00';\n      const time1 = `${sHours}:${sMinutes}:${seconds}`;\n      const dateFromPicker = datevalue;\n      const timeFromPicker = time1;\n      const datetime = `${datevalue} ${time1}`;\n      const dateParts = dateFromPicker.split('-');\n      const timeParts = timeFromPicker.split(':');\n      console.log(datevalue);\n      console.log(time1);\n      const localDate = new Date(dateParts[0], dateParts[1] - 1,\n        dateParts[2], timeParts[0], timeParts[1]);\n      const timestamp = localDate.getTime();\n      console.log(localDate);\n      console.log(timestamp);\n      return datetime;\n    },\n    timeCalc1(timevalue, datevalue) {\n      console.log(timevalue);\n      console.log(datevalue);\n      let hours = Number(timevalue.match(/^(\\d+)/)[1]);\n      const minutes = Number(timevalue.match(/:(\\d+)/)[1]);\n      const AMPM = timevalue.match(/\\s(.*)$/)[1];\n      if (AMPM === 'PM' && hours < 12) hours += 12;\n      if (AMPM === 'AM' && hours === 12) hours -= 12;\n      let sHours = hours.toString();\n      let sMinutes = minutes.toString();\n      if (hours < 10) sHours = `0${sHours}`;\n      if (minutes < 10) sMinutes = `0${sMinutes}`;\n      const time1 = `${sHours}:${sMinutes}`;\n      const dateFromPicker = datevalue;\n      const timeFromPicker = time1;\n      const dateParts = dateFromPicker.split('-');\n      const timeParts = timeFromPicker.split(':');\n      const localDate = new Date(dateParts[0], dateParts[1] - 1,\n        dateParts[2], timeParts[0], timeParts[1]);\n      return localDate;\n    },\n    getDuration() {\n      const fromdt = this.timeCalc1(this.addBookForm.fromtime, this.addBookForm.fromdate);\n      const todt = this.timeCalc1(this.addBookForm.totime, this.addBookForm.todate);\n      const hours = Math.floor(Math.abs(fromdt - todt) / 36e5);\n      console.log(fromdt);\n      console.log(todt);\n      console.log(hours);\n      return hours;\n    },\n    addBook(payload) {\n      const path = 'http://localhost:5000/bookings/range';\n      console.log(path);\n      axios.post(path, payload)\n        .then((resp) => {\n          this.message = '';\n          this.showMessage = true;\n          document.getElementById('response').innerHTML = resp.data;\n          this.$refs.bookingrangeModal.show();\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.log(error);\n        });\n    },\n    \n    initForm() {\n      this.addBookForm.country = '';\n      this.addBookForm.state = '';\n      this.addBookForm.city = '';\n      this.addBookForm.lots = '';\n      this.addBookForm.customertype = '';\n      this.addBookForm.vehicletype = '';\n      this.addBookForm.parkingtype = '';\n      this.addBookForm.fromdate = '';\n      this.addBookForm.fromtime = '';\n      this.addBookForm.todate = '';\n      this.addBookForm.totime = '';\n      this.addBookForm.actualdate = '';\n      this.addBookForm.actualtime = '';\n      this.addBookForm.stalls = '';\n      this.addBookForm.promocode = '';\n      this.addBookForm.interval = '';\n    },\n    onSubmit(evt) {\n      evt.preventDefault();\n      // this.$refs.addBookModal.hide();\n      const uid = Math.floor(Math.random() * 90000) + 10000;\n      let stallno = 1;\n      if (document.querySelector('#form-parkingtype-group').value === 'event') {\n        stallno = this.addBookForm.stalls;\n      }\n      if(this.addBookForm.actualtime == '' || this.addBookForm.actualdate == '') {\n        this.addBookForm.actualtime = this.addBookForm.totime;\n        this.addBookForm.actualdate = this.addBookForm.todate;\n      }\n      let promo = '';\n      if (this.addBookForm.checkedPromo == true && this.addBookForm.promocode != '') {\n        promo = this.addBookForm.promocode;\n      }\n      const payload = {\n        bookingid: uid,\n        country: 'USA',\n        currency: '$',\n        state: 'Newyork',\n        city: 'Newyork',\n        lotid: this.addBookForm.lots,\n        lotname: 'Wriglyville',\n        customertype: this.addBookForm.customertype,\n        vehicletype: this.addBookForm.vehicletype,\n        parkingtype: this.addBookForm.parkingtype,\n        fromtimestamp: this.timeCalc(this.addBookForm.fromtime, this.addBookForm.fromdate),\n        totimestamp: this.timeCalc(this.addBookForm.totime, this.addBookForm.todate),\n        stalls: stallno,\n        duration: this.getDuration(),\n        actualexittime: this.timeCalc(this.addBookForm.actualtime, this.addBookForm.actualdate),\n        promocode: promo,\n        interval: this.addBookForm.interval,\n      };\n      console.log(payload);\n        this.addBook(payload);\n        //  this.initForm();\n    },\n  },\n  created() {\n    // this.getBooks();\n  },\n};\n</script>\n"]}]}