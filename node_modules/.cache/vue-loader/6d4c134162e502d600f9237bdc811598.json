       v-model=\"rules.op1\" :name=\"`addRulesForm[${index}][op1]`\"\n                        placeholder=\"Enter op1\"\n                        >\n          </b-form-input>\n        </b-form-group>\n        <b-form-group id=\"form-operator-group\"\n                      label=\"Operator:\"\n                      label-for=\"form-operator-input\">\n            <b-form-input id=\"form-operator-input\"\n                          type=\"text\"\n                          v-model=\"rules.operator\" :name=\"`addRulesForm[${index}][operator]`\"\n                          placeholder=\"Enter operator\">\n            </b-form-input>\n          </b-form-group>\n        <b-form-group id=\"form-type-group\"\n                      label=\"Type:\"\n                      label-for=\"form-type-input\">\n            <b-form-input id=\"form-type-input\"\n                          type=\"text\"\n                          v-model=\"rules.type\" :name=\"`addRulesForm[${index}][type]`\"\n                          placeholder=\"Enter type\">\n            </b-form-input>\n          </b-form-group>\n        <b-form-group id=\"form-value-group\"\n                      label=\"Value:\"\n                      label-for=\"form-value-input\">\n            <b-form-input id=\"form-value-input\"\n                          type=\"text\"\n                          v-model=\"rules.value\" :name=\"`addRulesForm[${index}][value]`\"\n                          placeholder=\"Enter value\">\n            </b-form-input>\n          </b-form-group>\n        <b-form-group id=\"form-action-group\"\n                      label=\"Action:\"\n                      label-for=\"form-action-input\">\n            <b-form-select v-model=\"rules.action\" :name=\"`addRulesForm[${index}][action]`\"\n            id=\"form-action-group\">\n            <option value=\"\" selected>Please select action</option>\n            <option value=\"Promocode\">Promocode</option>\n            <option value=\"Occupancy Factor\">Occupancy Factor</option>\n            </b-form-select>\n            </b-form-group>\n        <b-form-group id=\"form-actionvalue-group\"\n                      label=\"Action value:\"\n                      label-for=\"form-actionvalue-input\">\n            <b-form-input id=\"form-actionvalue-input\"\n                          type=\"text\"\n                          v-model=\"rules.actionvalue\" :name=\"`addRulesForm[${index}][actionvalue]`\"\n                          placeholder=\"Enter action value\">\n            </b-form-input>\n          </b-form-group>\n        </div>\n        <div id=\"ifrules\" style=\"display:none;\" >\n        <h1 style=\"background: lightgray;font-size: 1.2rem;width: 460px;\n        padding-left: 35px;margin-bottom: 30px;margin-top: 20px;\">IF\n        <select v-model=\"addRulesForm.logicaloperator\"\n            name=\"`addRulesForm[0][logicaloperator]`\"\n            id=\"form-logicaloperator-group\">\n            <option value=\"NONE\" selected id=\"none\">NONE</option>\n            <option value=\"OR\" id=\"any\">ANY</option>\n            <option value=\"AND\">ALL</option>\n            </select>\n        OF THE IF RULES ARE MATCHED</h1>\n        </div>\n        <b-button-group style=\"display:inline;\">\n          <b-button type=\"submit\" variant=\"primary\"\n          style=\"width: 150px;margin-right: 160px;\">Submit</b-button>\n        <b-button @click=\"addmorerules()\" variant=\"danger\"\n          style=\"width: 150px;\">Add</b-button>\n        <b-button @click=\"remrules()\" variant=\"danger\" id=\"rem\"\n          style=\"width: 150px;display:none;\">Rem</b-button>\n        <!--  <b-button @click=\"addExperience\" v-on:click=\"seen = !seen\" variant=\"danger\"\n          style=\"width: 150px;\">Add</b-button>  -->\n        <!--  <b-button @click=\"removeExperience(index)\" variant=\"danger\"\n          style=\"float: right;width: 150px;\">Remove</b-button> -->\n        </b-button-group>\n      </b-form>\n      </div>\n    </v-modal>\n    <br><br>\n    <hr><br><br>\n    </div>\n    </div>\n  </div>\n</template>\n\n<script>\n\nimport axios from 'axios';\nimport Alert from './Alert.vue';\n\nexport default {\n  data: () => ({\n    addRulesForm: [\n      {\n        op1: '',\n        operator: '',\n        type: '',\n        value: '',\n        action: '',\n        actionvalue: '',\n        businessrule: '',\n      },\n    ],\n    message: '',\n    showMessage: false,\n  }),\n  components: {\n    alert: Alert,\n  },\n  methods: {\n    addmorerules(val) {\n      const newval = parseInt(val, 10);\n      const add = '#add';\n      const sub = '#sub';\n      const ifrules = '#ifrules';\n      document.querySelector(ifrules).style.display = 'block';\n      document.getElementById('none').style.display = 'none';\n      document.querySelector('#rem').style.display = 'block';\n      //  document.getElementById('form-logicaloperator-group').selectedIndex = 1;\n      document.getElementById('form-logicaloperator-group').options.selectedIndex = '1';\n      this.addRulesForm.push({\n        op1: '',\n        operator: '',\n        type: '',\n        value: '',\n        action: '',\n        actionvalue: '',\n      });\n      //  const val1 = newval + 1;\n      console.log(val);\n      if (newval >= 0) {\n        document.querySelector(`${add}${newval}`).style.display = 'none';\n        document.querySelector(`${sub}${newval}`).style.display = 'none';\n        document.querySelector(`${ifrules}${newval}`).style.display = 'none';\n      }\n    },\n    remrules() {\n      this.addRulesForm.splice(0, 1);\n    },\n    addBook(payload) {\n      const path = 'http://localhost:5000/rules';\n      axios.post(path, payload)\n        .then((res) => {\n          this.message = res.data;\n          this.showMessage = true;\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.log(error);\n        });\n    },\n    initForm() {\n      this.addRulesForm.op1 = '';\n      this.addRulesForm.operator = '';\n      this.addRulesForm.type = '';\n      this.addRulesForm.value = '';\n      this.addRulesForm.action = '';\n      this.addRulesForm.actionvalue = '';\n      this.addRulesForm.logicaloperator = '';\n    },\n    onSubmit(evt) {\n      evt.preventDefault();\n      const mainarray = this.addRulesForm;\n      const lot1 = this.addRulesForm.lot;\n      const validity1 = this.addRulesForm.validity;\n      let logicaloperator1 = this.addRulesForm.logicaloperator;\n      let formdata = {};\n      let arraydata = {};\n      if (mainarray.length === 1) {\n        logicaloperator1 = 'NONE';\n      }\n      for (let i = 0; i < mainarray.length; i += 1) {\n        //  const a = 'op1';\n        const operand1 = this.addRulesForm[i].op1;\n        const operator1 = this.addRulesForm[i].operator;\n        const type1 = this.addRulesForm[i].type;\n        const value1 = this.addRulesForm[i].value;\n        const action1 = this.addRulesForm[i].action;\n        const actionvalue1 = this.addRulesForm[i].actionvalue;\n        formdata = {};\n        const cond = 'cond';\n        const conditions = `${cond}${i + 1}`;\n        const operand2 = 'operand';\n        const operator2 = 'operator';\n        const type2 = 'type';\n        const value2 = 'value';\n        const action2 = 'action';\n        formdata[conditions] = {};\n        formdata[conditions][operand2] = operand1;\n        formdata[conditions][operator2] = operator1;\n        formdata[conditions][type2] = type1;\n        formdata[conditions][value2] = value1;\n        formdata[conditions][action2] = {};\n        formdata[conditions][action2][action1] = actionvalue1;\n        arraydata = Object.assign(arraydata, formdata);\n        alert(JSON.stringify(arraydata, null, 2));\n      }\n      const lot2 = 'lot';\n      const validity2 = 'validity';\n      const logicaloperator2 = 'logicaloperator';\n      const rulename = this.addRulesForm.businessrule;\n      const finalarray1 = {};\n      finalarray1[rulename] = arraydata;\n      //  finalarray1[rulename][lot2] = [];\n      finalarray1[rulename][lot2] = [lot1];\n      finalarray1[rulename][validity2] = validity1;\n      finalarray1[rulename][logicaloperator2] = logicaloperator1;\n      const payload = finalarray1;\n      console.log(payload);\n      this.addBook(payload);\n      this.initForm();\n    },\n    onSubmitUpdate(evt) {\n      evt.preventDefault();\n      this.$refs.editBookModal.hide();\n      let read = false;\n      if (this.editForm.read[0]) read = true;\n      const payload = {\n        title: this.editForm.title,\n        author: this.editForm.author,\n        read,\n      };\n      this.updateBook(payload, this.editForm.id);\n    },\n    onReset(evt) {\n      evt.preventDefault();\n      this.$refs.addBookModal.hide();\n      this.initForm();\n    },\n    onResetUpdate(evt) {\n      evt.preventDefault();\n      this.$refs.editBookModal.hide();\n      this.initForm(); // why?\n    },\n  },\n  created() {\n  },\n};\n</script>\n"]}]}